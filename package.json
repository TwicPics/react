{
  "name": "@twicpics/react",
  "description": "TwicPics components library for React.js",
  "version": "0.1.1",
  "author": "TwicPics",
  "license": "MIT",
  "main": "./dist/twicpics.js",
  "style": "./dist/twicpics.css",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react-component",
    "twicpics",
    "react-twicpics",
    "image"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/TwicPics/react.git"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "8.1.0",
    "css-loader": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.23.1",
    "mini-css-extract-plugin": "^1.3.9",
    "react": "^17.0.2",
    "style-loader": "^1.0.1",
    "webpack": "4.44.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "scripts": {
    "build": "webpack",
    "test": "eslint --ext .js,.jsx src"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "plugin:react/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "16.3"
      }
    },
    "rules": {}
  },
  "bugs": {
    "url": "https://github.com/TwicPics/react/issues"
  },
  "homepage": "https://github.com/TwicPics/react#readme",
  "publishConfig": {
    "access": "public"
  }
